pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git 'https://github.com/Pritam-Khergade/student-ui.git'
            }
        }
        stage('image&build') {
            steps {
                sh 'sudo yum install maven -y && sudo mvn clean package' 
            }    
        }
    }
}

